{
  "version": 2,
  "builds": [
    {
      "src": "packages/docs/package.json",
      "use": "@now/next@canary"
    }
  ],
  "name": "chakra-ui",
  "alias": "chakra-ui.com",
  "routes": [
    {
      "src": "^(.*)$",
      "dest": "packages/docs$1"
    },
    {
      "src": "www.chakra-ui.com/*",
      "status": 301,
      "headers": { "Location": "https://chakra-ui.com/$1" }
    }
  ],
  "scope": "chakra-ui"
}
