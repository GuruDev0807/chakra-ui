{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:jsx-a11y/recommended", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "jsx-a11y", "import", "jest", "react-hooks"],
  "rules": {
    "no-console": "off"
  }
}
