{
  "name": "@chakra-ui/core",
  "version": "0.4.0",
  "description": "Responsive and accessible React UI components built with React and Emotion",
  "main": "dist/cjs",
  "module": "dist/esm",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "nodemon --exec yarn build -e ts,tsx --ignore dist/ --ignore src/tests/ --ignore \"*.stories.tsx\"",
    "build": "concurrently yarn:build:*",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "build:esm": "tsc --module esnext --outDir dist/esm --declaration false",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --declaration false",
    "build:types": "tsc --emitDeclarationOnly --declaration true --declarationDir dist/types",
    "test:cov": "yarn test --coverage",
    "lint:src": "eslint src --ext .ts,.tsx --config ../../.eslintrc",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@chakra-ui/css-reset": "0.0.1",
    "@chakra-ui/focus-lock": "0.0.1",
    "@chakra-ui/live-region": "0.0.1",
    "@chakra-ui/popper": "0.0.1",
    "@chakra-ui/clickable": "0.0.1",
    "@chakra-ui/color": "0.0.1",
    "@chakra-ui/counter": "0.0.1",
    "@chakra-ui/descendant": "0.0.1",
    "@chakra-ui/portal": "0.0.1",
    "@chakra-ui/system": "0.0.1",
    "@chakra-ui/breadcrumb": "0.0.1",
    "@chakra-ui/collapse": "0.0.1",
    "@chakra-ui/editable": "0.0.1",
    "@chakra-ui/icon": "0.0.1",
    "@chakra-ui/image": "0.0.1",
    "@chakra-ui/media-query": "0.0.1",
    "@chakra-ui/menu": "0.0.1",
    "@chakra-ui/pin-input": "0.0.1",
    "@chakra-ui/progress": "0.0.1",
    "@chakra-ui/slider": "0.0.1",
    "@chakra-ui/tabs": "0.0.1",
    "@chakra-ui/toast": "0.0.1",
    "@chakra-ui/visually-hidden": "0.0.1",
    "@chakra-ui/avatar": "0.0.1",
    "@chakra-ui/checkbox": "0.0.1",
    "@chakra-ui/close-button": "0.0.1",
    "@chakra-ui/form-control": "0.0.1",
    "@chakra-ui/radio": "0.0.1",
    "@chakra-ui/spinner": "0.0.1",
    "@chakra-ui/stat": "0.0.1",
    "@chakra-ui/tag": "0.0.1",
    "@chakra-ui/tooltip": "0.0.1",
    "@chakra-ui/accordion": "0.0.1",
    "@chakra-ui/alert": "0.0.1",
    "@chakra-ui/button": "0.0.1",
    "@chakra-ui/input": "0.0.1",
    "@chakra-ui/modal": "0.0.1",
    "@chakra-ui/number-input": "0.0.1",
    "@chakra-ui/popover": "0.0.1",
    "@chakra-ui/select": "0.0.1",
    "@chakra-ui/switch": "0.0.1",
    "@chakra-ui/textarea": "0.0.1",
    "@chakra-ui/alert-dialog": "0.0.1",
    "@chakra-ui/drawer": "0.0.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "license": "MIT",
  "homepage": "https://chakra-ui.com/",
  "repository": "https://github.com/chakra-ui/chakra-ui",
  "keywords": [
    "react",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "emotion",
    "library",
    "design-system"
  ],
  "gitHead": "bf23c9514f184e58fb8f652bfaa38826d1b88906"
}
