{
  "version": 2,
  "name": "chakra-ui",
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "alias": ["chakra-ui.com", "chakra-ui"],
  "scope": "chakra-ui",
  "routes": [
    {
      "src": "www.chakra-ui.com/*",
      "status": 301,
      "headers": { "Location": "https://chakra-ui.com/$1" }
    }
  ]
}
